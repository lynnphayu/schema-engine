{
  "tables": {
    "users": {
      "id": "serial",
      "username": "varchar(50)",
      "email": "varchar(255)",
      "password_hash": "varchar(255)",
      "full_name": "varchar(100)",
      "is_active": "boolean",
      "created_at": "timestamp",
      "updated_at": "timestamp"
    },
    "products": {
      "id": "serial",
      "name": "varchar(100)",
      "description": "text",
      "price": "numeric(10,2)",
      "stock_quantity": "integer",
      "category": "varchar(50)",
      "is_available": "boolean",
      "metadata": "jsonb",
      "created_at": "timestamp",
      "updated_at": "timestamp"
    },
    "orders": {
      "id": "serial",
      "user_id": "integer",
      "order_date": "timestamp",
      "total_amount": "numeric(12,2)",
      "status": "varchar(20)",
      "shipping_address": "text",
      "tracking_number": "varchar(50)",
      "created_at": "timestamp",
      "updated_at": "timestamp"
    },
    "order_items": {
      "id": "serial",
      "order_id": "integer",
      "product_id": "integer",
      "quantity": "integer",
      "unit_price": "numeric(10,2)",
      "subtotal": "numeric(10,2)",
      "created_at": "timestamp"
    }
  }
}
